generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @unique
  email       String?
  balance     Decimal @default(0)
  address     String?
  displayName String?
  image       String?

  @@map("users")
}
